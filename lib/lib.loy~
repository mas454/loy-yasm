(require "lib/read.rb")
(reqiore "lib/loy.rb")
(def string? (a)
     (a.kind_of? String))
(def number? (o)
     (o.kind_of? Numeric))
(def symbol? (o)
     (o.kind_of? Symbol))

(def eq? (a b)
     (a.equal? b))

(def null? (a)
     (a.nil?))

(= S_READER (Reader.new))

(def read (io)
     	 (S_READER io))

(def (open_input_file path)
     (open path "r"))

(def (map fn lis)
     (if (null? lis)
	 nil
	 (cons 
	  (fn.call (car lis))
	  (map fn (cdr lis)))))

