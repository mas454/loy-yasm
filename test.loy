(macro (mcall fn x *args)
       `(begin
	 (macro_val_print ,x)
	 (rasm ".")
	 (macro_val_print ,fn)
	 (rasm "(")
	 ,@(map (lambda (arg)
		  `(begin
		    (macro_val_print ,arg)
		    (rasm ","))) (a2l args))
	 (rasm ")\n")))
(defclass Test
  (def (ts a)
       a))
(= b (Test.new))

(mcall to_s (mcall ts b 40))

